    "discord-player": "^7.1.0",
    "discord-player-youtubei": "^1.3.2",
    "discord.js": "^14.16.1",
    "dotenv": "^16.0.1",
    "ffmpeg-static": "^5.2.0",
    "firebase": "^12.0.0",
    "mediaplex": "^1.0.0",
    "nodemailer": "^6.10.0",
    "steamapi": "^3.0.12"
  }
