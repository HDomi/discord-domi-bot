{
  "name": "domi_bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "docker build -t domi-bot .",
    "run": "docker run --env-file .env.development -p 5539:5539 domi-bot",
    "clean": "docker stop $(docker ps -q --filter ancestor=domi-bot) 2>/dev/null || true && docker rm $(docker ps -a -q --filter ancestor=domi-bot) 2>/dev/null || true && docker rmi domi-bot 2>/dev/null || true",
    "rebuild": "npm run clean && npm run build && npm run run",
    "del-images": "docker rmi $(docker images -q)",
    "del-containers": "docker rm $(docker ps -a -q)"
  },
  "author": "",
